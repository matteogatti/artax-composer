{
  "name": "pensiero/artax-composer",
  "description": "Wrapper of the Artax library",
  "type": "library",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/pensiero/artax-composer",
  "keywords": [
    "zf2",
    "zend-framework",
    "amphp",
    "artax",
    "curl"
  ],
  "authors": [
    {
      "name": "Oscar Fanelli",
      "email": "oscar.fanelli@gmail.com",
      "homepage": "http://www.oscarfanelli.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.0",
    "zendframework/zend-cache": "~2.7",
    "zendframework/zend-modulemanager": "~2.7",
    "zendframework/zend-mvc": "~2.7",
    "php-http/guzzle6-adapter": "1.*",
    "amphp/http-client": "^4.2",
    "ext-json": "*"
  },
  "autoload": {
    "psr-0": {
      "ArtaxComposer\\": "src/"
    },
    "classmap": [
      "./Module.php"
    ]
  },
  "require-dev": {
    "phpunit/phpunit": "^8",
    "symfony/var-dumper": "^5.0"
  }
}
